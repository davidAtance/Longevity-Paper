pol_t <- cbind.data.frame(Age = pol_dea$Age, Year = pol_dea$Year, Exp = (pol_exp$Total + (1 -pol$ax)*pol_dea$Total), Deaths = pol_dea$Total, qxt = qxt_pol_total, ax = pol$ax)

