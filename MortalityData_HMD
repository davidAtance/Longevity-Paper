               KOR = list(hombres = kor_m, mujeres = kor_f, total = kor_t),
               LTU = list(hombres =ltu_m, mujeres = ltu_f, total = ltu_t),
               LUX = list(hombres = lux_m, mujeres = lux_f, total = lux_t),
